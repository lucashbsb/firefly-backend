{
  "root": true,
  "env": {
    "es2021": true,
    "node": true
  },
  "parserOptions": {
    "ecmaVersion": 2021,
    "sourceType": "module"
  },
  "extends": [
    "eslint:recommended",
    "plugin:import/recommended",
    "plugin:security/recommended",
    "prettier"
  ],
  "plugins": ["import", "security"],
  "rules": {
    "import/no-unresolved": "off"
  }
}
